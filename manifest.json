{
    "manifest_version": 3,
    "name": "GOTIcket",
    "description": "GO TO for Jira tickets",
    "version": "1.2",
    "permissions":
    [
        "tabs",
        "contextMenus",
        "activeTab",
        "scripting"
    ],
        "commands":
    {
        "_execute_action":
        {
            "suggested_key":
            {
                "default": "Ctrl+J",
                "mac": "Command+J"
            },
            "description": "Start the extension"
        }
    },
    "action":
    {
        "default_popup": "popup.html",
        "default_icon": "favicon.png"
    },
    "background":
    {
        "service_worker": "background.js"
    },
    "icons":
    {
        "16": "favicon.png",
        "48": "favicon.png",
        "128": "favicon.png"
    }
}