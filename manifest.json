{
  "manifest_version": 3,
  "name": "GOTIcket",
  "description": "GO TO for Jira tickets",
  "version": "1.3",
  "permissions":[
    "tabs",
    "contextMenus",
    "activeTab",
    "scripting",
    "storage"
  ],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Alt+Shift+J",
        "mac": "Command+J"
      },
      "description": "Start the extension"
    }
  },
  "action": {
    "default_popup": "./pages/popup/index.html",
    "default_icon": "./assets/img/favicon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "./assets/img/favicon.png",
    "48": "./assets/img/favicon.png",
    "128": "./assets/img/favicon.png"
  },
  "options_ui": {
    "page": "./pages/options/index.html",
    "open_in_tab": true
  }
}